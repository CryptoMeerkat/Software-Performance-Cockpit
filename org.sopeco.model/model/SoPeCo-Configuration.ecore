<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="configuration"
    nsURI="http://sopeco.org/configuration" nsPrefix="org.sopeco.core.model">
  <eClassifiers xsi:type="ecore:EClass" name="ScenarioDefinition">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="measurementEnvironmentDefinition"
        lowerBound="1" eType="#//environment/MeasurementEnvironmentDefinition" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="measurementSpecification"
        lowerBound="1" eType="#//measurements/MeasurementSpecification" containment="true"/>
  </eClassifiers>
  <eSubpackages name="environment" nsURI="http://sopeco.org/configuration/environment"
      nsPrefix="environment">
    <eClassifiers xsi:type="ecore:EClass" name="MeasurementEnvironmentDefinition"
        eSuperTypes="#//common/Serializable">
      <eStructuralFeatures xsi:type="ecore:EReference" name="root" lowerBound="1"
          eType="#//environment/ParameterNamespace" containment="true"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="ParameterNamespace" eSuperTypes="#//common/Serializable">
      <eStructuralFeatures xsi:type="ecore:EReference" name="children" upperBound="-1"
          eType="#//environment/ParameterNamespace" containment="true"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="parameters" upperBound="-1"
          eType="#//environment/ParameterDefinition" containment="true">
        <eAnnotations source="http:///org/eclipse/emf/ecore/util/ExtendedMetaData">
          <details key="namespace" value=""/>
        </eAnnotations>
      </eStructuralFeatures>
      <eStructuralFeatures xsi:type="ecore:EReference" name="parent" eType="#//environment/ParameterNamespace"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="ParameterDefinition" eSuperTypes="#//common/Serializable">
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" lowerBound="1"
          eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="type" lowerBound="1"
          eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="role" lowerBound="1"
          eType="#//environment/ParameterRole"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="fullName" unique="false"
          eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"
          changeable="false" volatile="true" transient="true" unsettable="true" derived="true">
        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
          <details key="documentation" value="This is a 'derived' attribute computed out of the namespace structure and the parameter's name."/>
        </eAnnotations>
      </eStructuralFeatures>
      <eStructuralFeatures xsi:type="ecore:EReference" name="namespace" lowerBound="1"
          eType="#//environment/ParameterNamespace"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EEnum" name="ParameterRole">
      <eLiterals name="INPUT"/>
      <eLiterals name="OBSERVATION" value="1"/>
    </eClassifiers>
  </eSubpackages>
  <eSubpackages name="measurements" nsURI="http://sopeco.org/configuration/measurements"
      nsPrefix="measurements">
    <eClassifiers xsi:type="ecore:EClass" name="MeasurementSpecification">
      <eStructuralFeatures xsi:type="ecore:EReference" name="experimentSeriesDefinitions"
          lowerBound="1" upperBound="-1" eType="#//measurements/ExperimentSeriesDefinition"
          containment="true"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="initializationAssignemts"
          upperBound="-1" eType="#//measurements/ConstantValueAssignment" containment="true"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="ExperimentSeriesDefinition" eSuperTypes="#//common/Serializable">
      <eStructuralFeatures xsi:type="ecore:EReference" name="explorationStrategy"
          lowerBound="1" eType="#//measurements/ExplorationStrategy" containment="true"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="experimentAssignments"
          lowerBound="1" upperBound="-1" eType="#//measurements/ParameterValueAssignment"
          containment="true"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="experimentTerminationCondition"
          lowerBound="1" eType="#//measurements/ExperimentTerminationCondition" containment="true"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="preperationAssignments"
          upperBound="-1" eType="#//measurements/ConstantValueAssignment" containment="true"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" lowerBound="1"
          eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="ParameterValueAssignment" abstract="true"
        eSuperTypes="#//common/Serializable">
      <eStructuralFeatures xsi:type="ecore:EReference" name="parameter" lowerBound="1"
          eType="#//environment/ParameterDefinition"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="ExplorationStrategy" eSuperTypes="#//measurements/ExtensibleElement">
      <eStructuralFeatures xsi:type="ecore:EReference" name="analysisConfigurations"
          upperBound="-1" eType="#//analysis/AnalysisConfiguration" containment="true"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="ExperimentTerminationCondition" abstract="true"
        eSuperTypes="#//common/Serializable"/>
    <eClassifiers xsi:type="ecore:EClass" name="ConfigurationNode" instanceClassName="java.util.Map$Entry"
        eSuperTypes="#//common/Serializable">
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="key" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="value" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="ExtensibleElement" abstract="true"
        eSuperTypes="#//common/Serializable">
      <eStructuralFeatures xsi:type="ecore:EReference" name="configuration" upperBound="-1"
          eType="#//measurements/ConfigurationNode" containment="true"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="NumberOfRepetitions" eSuperTypes="#//measurements/ExperimentTerminationCondition">
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="numberOfRepetitions"
          eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//ELong"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="TimeOut" eSuperTypes="#//measurements/ExperimentTerminationCondition">
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="maxDuration" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//ELong"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="ConstantValueAssignment" eSuperTypes="#//measurements/ParameterValueAssignment">
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="value" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="DynamicValueAssignment" eSuperTypes="#//measurements/ExtensibleElement #//measurements/ParameterValueAssignment"/>
  </eSubpackages>
  <eSubpackages name="analysis" nsURI="http://sopeco.org/configuration/analysis" nsPrefix="analysis">
    <eClassifiers xsi:type="ecore:EClass" name="AnalysisConfiguration" eSuperTypes="#//measurements/ExtensibleElement"/>
  </eSubpackages>
  <eSubpackages name="common" nsURI="http://sopeco.org/configuration/common" nsPrefix="common">
    <eClassifiers xsi:type="ecore:EClass" name="Serializable" instanceClassName="java.io.Serializable"
        abstract="true" interface="true"/>
    <eClassifiers xsi:type="ecore:EClass" name="SerializableEObject" abstract="true"
        interface="true" eSuperTypes="#//common/Serializable #//common/EObject"/>
    <eClassifiers xsi:type="ecore:EClass" name="EObject" instanceClassName="org.eclipse.emf.ecore.EObject"
        abstract="true" interface="true"/>
  </eSubpackages>
</ecore:EPackage>
